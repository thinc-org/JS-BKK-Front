{
  "name": "bkk-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn dev:next\" \"yarn dev:style\"",
    "dev:next": "next dev",
    "dev:style": "postcss ./styles/tailwind.css -o ./styles/index.css -w -m",
    "build": "yarn build:style && yarn build:next",
    "build:next": "next build",
    "build:style": "postcss ./styles/tailwind.css -o ./styles/index.css",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "yarn prettier --write \"./**/*.ts*\"",
    "export": "next export"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.0",
    "date-fns": "^2.8.1",
    "firebase": "7.7.0",
    "mobx": "^5.15.1",
    "mobx-react-lite": "^1.5.2",
    "next": "9.1.6",
    "qrcode.react": "^1.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-id-generator": "^3.0.0",
    "react-hook-form": "^4.7.1",
    "react-qr-reader": "^2.2.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@types/node": "^12.12.21",
    "@types/qrcode.react": "^1.0.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.5",
    "@types/react-qr-reader": "^2.1.2",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "autoprefixer": "^9.7.3",
    "concurrently": "^5.0.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "postcss-cli": "^7.1.0",
    "prettier": "^1.19.1",
    "tailwindcss": "^1.1.4",
    "typescript": "^3.7.4"
  }
}
